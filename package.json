{
  "name": "s7orybook",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "test:storybook": "test-storybook",
    "test:coverage": "test-storybook --coverage",
    "coverage-report": "test-storybook --coverage && cp coverage/storybook/coverage-storybook.json coverage/coverage-storybook.json && nyc report --reporter=html -t coverage --report-dir coverage && npx serve coverage/",
    "lint": "eslint . --ext ts,tsx",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "clean": "rimraf coverage .cache storybook-static artifacts **/__snapshots__"
  },
  "dependencies": {
    "clsx": "^1.2.1",
